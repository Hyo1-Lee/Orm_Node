
            <!--우측 콘텐츠 헤더영역 -->
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>신규 채널 등록</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/Home/Index">Home</a> > 
                        </li>
                        <li>
                            <a> 신규 채널</a> > 
                        </li>
                        <li class="active">
                            <strong> 등록</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <!--우측 콘텐츠 영역 -->
            <form action="/channel/modify/<%=channel.channel_id%>" id='articleForm' method="post">
                <div class="wrapper wrapper-content">
                    <div class="ibox-content m-b-sm border-bottom">

                        <!-- 게시판 영역 -->
                        <div class="row">
                            <div class="col-sm-2">
                                <div class="form-group">
                                    <label class="control-label">채널</label>
                                    <select name="categoryCode" id="categoryCode" class="form-control">
                                        <option value="0" <% if( channel.category_code == "0"){ %> selected <% } %>>선택</option>
                                        <option value="1" <% if( channel.category_code == "1"){ %> selected <% } %>>공지채널</option>
                                        <option value="2" <% if( channel.category_code == "2"){ %> selected <% } %>>잡담채널</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <!-- 글 제목 영역 -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label class="control-label">채널이름</label>
                                    <input type="text" name="channelName" id="channelName" class="form-control" value="<%=channel.channel_name%>">
                                </div>
                            </div>
                        </div>

                        <!-- 글 내용 입력 영역 -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label class="control-label">채널설명</label>
                                    <textarea class="form-control" name='channelDesc' id='channelDesc' rows="10" cols="5"><%= channel.channel_desc %></textarea>
                                </div>
                            </div>
                        </div>

                        <!-- 게시글 유형, 게시 여부, 등록자 입력 영역 -->
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">동시채널접속자수</label>
                                    <input type="text" name="userLimit" id="userLimit" class="form-control" value="<%=channel.user_limit%>">

                                    </select>
                                </div>
                            </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">게시여부</label>
                                        <select name="channelStateCode" id="channelStateCode" class="form-control" >
                                            <option value="9" <% if( channel.channel_state_code == "9"){ %> selected <% } %>>선택</option>
                                            <option value="1" <% if( channel.channel_state_code == "1"){ %> selected <% } %>>게시함</option>
                                            <option value="0" <% if( channel.channel_state_code == "0"){ %> selected <% } %>>게시안함</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">등록아이디</label>
                                        <input type="text" name="regMemberId" id="regMemberId" class="form-control" value="<%=channel.reg_member_id%>">
                                    </div>
                                </div>
                        </div>

                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">수정</button>
                            <!-- btn-danger: 빨간색 버튼 -->
                            <button type="button" id="btnDelete" class="btn btn-danger">삭제</button>
                            <a href="/channel/list" class="btn btn-info">목록</a>
                        </div>

                    </div>
                </div>
            </form>



    <script>


    </script>

